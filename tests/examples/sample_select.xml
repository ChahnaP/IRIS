<parameters>
	<requestType>select</requestType>
	<fields>
		<field>*</field>
		<field>pageID</field>
	</fields>
	<table>pages</table>
	<where>
			<logic type="and">
				<field operator="gt">
					<name>
						pageID
					</name>
					<value>
						2000
					</value>
				</field>
				<field operator="lt">
					<name>
						pageID
					</name>
					<value>
						2500
					</value>
				</field>
				<logic type="not">
					<logic type="or">
						<field operator="eq">
							<name>
								userID
							</name>
							<value>
								5
							</value>
						</field>
						<field operator="eq">
							<name>
								result
							</name>
							<value>
								TRUE
							</value>
						</field>
					</logic>
				</logic>
			</logic>
	</where>
	<orderby type="desc">
		<field>url</field>
	</orderby>
	<limit>
		10
	</limit>
</parameters>

